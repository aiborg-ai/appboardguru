{
  "name": "appboardguru-mobile",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "type-check": "tsc --noEmit",
    "build:android:debug": "cd android && ./gradlew assembleDebug",
    "build:android:release": "cd android && ./gradlew bundleRelease",
    "build:ios:debug": "xcodebuild -workspace ios/AppBoardGuruMobile.xcworkspace -scheme AppBoardGuruMobile -configuration Debug -destination generic/platform=iOS",
    "build:ios:release": "xcodebuild -workspace ios/AppBoardGuruMobile.xcworkspace -scheme AppBoardGuruMobile -configuration Release -destination generic/platform=iOS -archivePath AppBoardGuruMobile.xcarchive archive",
    "test:e2e": "detox test",
    "test:e2e:build": "detox build",
    "test:e2e:android": "detox test --configuration android.emu.debug",
    "test:e2e:ios": "detox test --configuration ios.sim.debug",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:unit": "jest src/",
    "test:integration": "jest __tests__/integration/",
    "test:performance": "jest __tests__/performance/",
    "security:scan": "npx audit-ci --config audit-ci.json",
    "bundle:analyze": "npx react-native-bundle-visualizer",
    "generate:icons": "node scripts/generate-icons.js",
    "generate:splash": "node scripts/generate-splash.js",
    "sync:types": "cp ../src/types/branded.ts src/types/shared/branded.ts && cp ../src/lib/result/index.ts src/types/shared/result.ts",
    "postinstall": "npx jetify && npm run sync:types",
    "clean": "cd android && ./gradlew clean && cd ../ios && xcodebuild clean",
    "reset": "npx react-native-clean-project"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.23.1",
    "@react-native-community/netinfo": "^11.3.1",
    "@react-native-firebase/app": "^19.2.2",
    "@react-native-firebase/messaging": "^19.2.2",
    "@react-native-masked-view/masked-view": "^0.3.1",
    "@react-navigation/bottom-tabs": "^6.5.20",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/native-stack": "^6.9.26",
    "@react-navigation/stack": "^6.3.29",
    "@tanstack/react-query": "^5.85.5",
    "@supabase/supabase-js": "^2.55.0",
    "@supabase/ssr": "^0.6.1",
    "react": "18.2.0",
    "react-native": "0.74.3",
    "react-native-background-job": "^0.2.0",
    "react-native-biometrics": "^3.0.1",
    "react-native-config": "^1.5.1",
    "react-native-device-info": "^10.13.1",
    "react-native-encrypted-storage": "^4.0.3",
    "react-native-fs": "^2.20.0",
    "react-native-gesture-handler": "^2.16.2",
    "react-native-keychain": "^8.2.0",
    "react-native-permissions": "^4.1.5",
    "react-native-reanimated": "^3.10.1",
    "react-native-safe-area-context": "^4.10.5",
    "react-native-screens": "^3.31.1",
    "react-native-shared-element": "^0.8.8",
    "react-native-sqlite-storage": "^6.0.1",
    "react-native-super-grid": "^5.0.0",
    "react-native-svg": "^15.2.0",
    "react-native-vector-icons": "^10.1.0",
    "react-native-webview": "^13.8.6",
    "watermelondb": "^0.27.1",
    "zustand": "^5.0.8",
    "zod": "^4.0.17",
    "react-native-tcp-socket": "^6.0.6",
    "react-native-ssl-pinning": "^1.5.1",
    "react-native-jailbreak-detector": "^1.0.1",
    "react-native-root-detection": "^1.0.0",
    "react-native-document-picker": "^9.1.1",
    "react-native-pdf": "^6.7.4",
    "react-native-image-picker": "^7.1.2",
    "react-native-camera": "^4.2.1",
    "react-native-voice": "^3.2.4",
    "react-native-sound": "^0.11.2",
    "react-native-haptic-feedback": "^2.2.0",
    "react-native-orientation-locker": "^1.7.0",
    "date-fns": "^4.1.0",
    "lodash": "^4.17.21",
    "react-hook-form": "^7.62.0",
    "react-native-toast-message": "^2.2.0",
    "react-native-modal": "^13.0.1",
    "react-native-picker-select": "^9.1.3",
    "react-native-date-picker": "^5.0.1",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-blur": "^4.4.0",
    "react-native-animatable": "^1.4.0",
    "react-native-draggable-flatlist": "^4.0.1",
    "react-native-super-grid": "^5.0.0",
    "socket.io-client": "^4.8.1",
    "uuid": "^11.1.0",
    "nanoid": "^5.1.5",
    "ts-pattern": "^5.8.0",
    "immer": "^10.1.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "tailwind-merge": "^3.3.1",
    "react-native-tailwindcss": "^1.1.11",
    "nativewind": "^2.0.11"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.74.85",
    "@react-native/eslint-config": "0.74.85",
    "@react-native/metro-config": "0.74.85",
    "@react-native/typescript-config": "0.74.85",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.6",
    "@types/react": "^18.2.6",
    "@types/react-native": "^0.73.0",
    "@types/react-test-renderer": "^18.0.0",
    "@types/uuid": "^11.1.0",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "@testing-library/react-native": "^12.7.2",
    "@testing-library/jest-native": "^5.4.3",
    "babel-jest": "^29.6.3",
    "detox": "^20.19.3",
    "eslint": "^8.19.0",
    "eslint-plugin-react-native": "^4.1.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "jest": "^29.6.3",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "2.8.8",
    "react-test-renderer": "18.2.0",
    "typescript": "5.0.4",
    "flipper-plugin-react-query": "^1.0.0",
    "react-native-clean-project": "^4.0.3",
    "react-native-bundle-visualizer": "^3.1.3",
    "audit-ci": "^7.1.0"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@10.0.0",
  "resolutions": {
    "react": "18.2.0",
    "react-native": "0.74.3"
  },
  "jest": {
    "preset": "react-native",
    "setupFilesAfterEnv": ["<rootDir>/src/utils/test-setup.ts"],
    "testMatch": ["**/__tests__/**/*.(test|spec).(ts|tsx|js|jsx)"],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.d.ts",
      "!src/**/__tests__/**",
      "!src/types/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "moduleNameMapping": {
      "@/(.*)$": "<rootDir>/src/$1",
      "@shared/(.*)$": "<rootDir>/../src/$1"
    }
  }
}