# BoardGuru Staging Environment Secrets Template
# Copy this file to staging-secrets.env and fill in actual values
# DO NOT commit the actual secrets file to version control

# Database Configuration
DATABASE_URL="postgresql://username:password@hostname:5432/boardguru_staging"
DATABASE_PASSWORD="your-secure-database-password"

# Redis Configuration
REDIS_URL="redis://username:password@hostname:6379"
REDIS_PASSWORD="your-redis-password"

# NextAuth Configuration
NEXTAUTH_SECRET="your-nextauth-secret-key-minimum-32-characters"
NEXTAUTH_URL="https://staging.boardguru.ai"

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL="https://your-project.supabase.co"
NEXT_PUBLIC_SUPABASE_ANON_KEY="your-supabase-anon-key"
SUPABASE_SERVICE_ROLE_KEY="your-supabase-service-role-key"

# Email SMTP Configuration
SMTP_HOST="smtp.gmail.com"
SMTP_PORT="587"
SMTP_USER="your-smtp-username@gmail.com"
SMTP_PASS="your-app-specific-password"
SMTP_SECURE="true"

# AI API Keys
ANTHROPIC_API_KEY="your-anthropic-api-key"
OPENAI_API_KEY="your-openai-api-key"

# AWS Credentials (if not using IAM roles)
AWS_ACCESS_KEY_ID="your-aws-access-key-id"
AWS_SECRET_ACCESS_KEY="your-aws-secret-access-key"
AWS_REGION="us-east-1"

# S3 Storage Configuration
AWS_S3_BUCKET="boardguru-staging-files"
AWS_S3_REGION="us-east-1"

# File Upload Configuration
MAX_FILE_SIZE="50MB"
ALLOWED_FILE_TYPES="pdf,docx,pptx,xlsx,txt"

# Security Configuration
ENCRYPTION_KEY="your-32-character-encryption-key"
JWT_SECRET="your-jwt-secret-key"
SESSION_SECRET="your-session-secret-key"

# External Service API Keys
STRIPE_SECRET_KEY="sk_test_your_stripe_secret_key"
STRIPE_PUBLISHABLE_KEY="pk_test_your_stripe_publishable_key"
STRIPE_WEBHOOK_SECRET="whsec_your_webhook_secret"

# Monitoring and Logging
SENTRY_DSN="https://your-sentry-dsn@sentry.io/project-id"
NEW_RELIC_LICENSE_KEY="your-new-relic-license-key"
DATADOG_API_KEY="your-datadog-api-key"

# Third-party Integrations
ZOOM_API_KEY="your-zoom-api-key"
ZOOM_API_SECRET="your-zoom-api-secret"
SLACK_BOT_TOKEN="xoxb-your-slack-bot-token"
SLACK_WEBHOOK_URL="https://hooks.slack.com/services/your/webhook/url"

# Feature Flags Service
LAUNCHDARKLY_SDK_KEY="your-launchdarkly-sdk-key"
SPLIT_API_KEY="your-split-api-key"

# Analytics
GOOGLE_ANALYTICS_ID="GA-your-tracking-id"
MIXPANEL_TOKEN="your-mixpanel-token"

# SSL/TLS Certificates (base64 encoded)
TLS_CERT_BASE64="base64-encoded-certificate"
TLS_KEY_BASE64="base64-encoded-private-key"

# OAuth Providers
GOOGLE_CLIENT_ID="your-google-oauth-client-id"
GOOGLE_CLIENT_SECRET="your-google-oauth-client-secret"
MICROSOFT_CLIENT_ID="your-microsoft-oauth-client-id"
MICROSOFT_CLIENT_SECRET="your-microsoft-oauth-client-secret"

# Backup Configuration
BACKUP_ENCRYPTION_KEY="your-backup-encryption-key"
BACKUP_S3_BUCKET="boardguru-staging-backups"
BACKUP_RETENTION_DAYS="30"

# Rate Limiting
RATE_LIMIT_REDIS_URL="redis://localhost:6379"
RATE_LIMIT_MAX_REQUESTS="1000"
RATE_LIMIT_WINDOW_MS="900000"

# CDN Configuration
CLOUDFLARE_API_TOKEN="your-cloudflare-api-token"
CLOUDFLARE_ZONE_ID="your-cloudflare-zone-id"

# Queue Management
QUEUE_REDIS_URL="redis://localhost:6379"
QUEUE_CONCURRENCY="5"

# Webhook Secrets
GITHUB_WEBHOOK_SECRET="your-github-webhook-secret"
STRIPE_WEBHOOK_SECRET="whsec_your_stripe_webhook_secret"

# Development and Testing
TEST_DATABASE_URL="postgresql://test:test@localhost:5432/boardguru_test"
TEST_ADMIN_EMAIL="test-admin@boardguru.ai"
TEST_USER_PASSWORD="test-password-123"

# Compliance and Security
GDPR_ENCRYPTION_KEY="your-gdpr-encryption-key"
AUDIT_LOG_ENCRYPTION_KEY="your-audit-log-encryption-key"
DATA_RETENTION_DAYS="365"

# Performance Monitoring
APPLICATION_INSIGHTS_INSTRUMENTATION_KEY="your-app-insights-key"
PROMETHEUS_PUSH_GATEWAY="https://your-prometheus-gateway"

# Environment Metadata
ENVIRONMENT="staging"
VERSION="v1.0.0"
BUILD_NUMBER="staging-001"
DEPLOYMENT_DATE="2024-01-01"
MAINTAINER_EMAIL="hirendra.vikram@boardguru.ai"